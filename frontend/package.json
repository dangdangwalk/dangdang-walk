    },
    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "build:prod": "FAST_REFRESH=false craco build",
        "test": "vitest",
        "eject": "react-scripts eject",
        "typecheck": "tsc --noEmit --skipLibCheck",
