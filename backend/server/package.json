{
    "name": "dangdangwalk-server",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "cross-env NODE_ENV=local nest start --watch",
        "start:dev": "cross-env NODE_ENV=dev nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "cross-env NODE_ENV=prod node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "cross-env NODE_ENV=test jest",
        "test:watch": "cross-env NODE_ENV=test jest --watch",
        "test:cov": "cross-env NODE_ENV=test jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "cross-env NODE_ENV=test jest --config ./test/jest-e2e.json --maxWorkers=1",
        "test:e2e-watch": "cross-env NODE_ENV=test jest --watch --config ./test/jest-e2e.json --maxWorkers=1",
        "test:only-changed": "cross-env NODE_ENV=test jest --onlyChanged",
        "test:response-time": "cross-env NODE_ENV=test ts-node ./test/performance/response-time.ts",
        "test:insert": "cross-env NODE_ENV=test ts-node ./test/performance/insert-test-data.ts",
        "test:clear": "cross-env NODE_ENV=test ts-node ./test/performance/clear-database.ts",
        "prepare": "cd .. && husky backend/.husky"
    },
    "lint-staged": {
        "*.{ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ]
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.577.0",
        "@aws-sdk/s3-request-presigner": "^3.577.0",
        "@nestjs/axios": "^3.0.2",
        "@nestjs/common": "^10.3.5",
        "@nestjs/config": "^3.2.2",
        "@nestjs/core": "^10.3.5",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/platform-express": "^10.3.5",
        "@nestjs/typeorm": "^10.0.2",
        "@willsoto/nestjs-prometheus": "^6.0.0",
        "app-root-path": "^3.1.0",
        "axios": "^1.6.8",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.1",
        "cookie-parser": "^1.4.6",
        "cross-env": "^7.0.3",
        "nest-winston": "^1.9.4",
        "prom-client": "^15.1.2",
        "reflect-metadata": "^0.1.14",
        "rxjs": "^7.8.1",
        "typeorm-extension": "^3.5.1",
        "typeorm-transactional": "^0.5.0",
        "uuid": "^9.0.1",
        "winston": "^3.13.0",
        "winston-daily-rotate-file": "^5.0.0"
    },
    "devDependencies": {
        "husky": "^9.0.11",
        "@nestjs/cli": "^10.3.2",
        "@nestjs/schematics": "^10.1.1",
        "@nestjs/testing": "^10.3.8",
        "@swc/cli": "^0.3.10",
        "@swc/core": "^1.5.0",
        "@swc/jest": "^0.2.29",
        "@types/cookie-parser": "^1.4.7",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/jest-plugin-context": "^2.9.7",
        "@types/jest-when": "^3.5.5",
        "@types/node": "^20.11.30",
        "@types/seedrandom": "^3.0.8",
        "@types/supertest": "^6.0.2",
        "@types/uuid": "^9.0.8",
        "jest": "^29.7.0",
        "jest-plugin-context": "^2.9.0",
        "jest-when": "^3.6.0",
        "lint-staged": "^15.2.2",
        "mysql2": "^3.9.4",
        "prettier": "^3.2.5",
        "seedrandom": "^3.0.5",
        "source-map-support": "^0.5.21",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.2",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typeorm": "^0.3.20",
        "typescript": "^5.4.3"
    }
}
